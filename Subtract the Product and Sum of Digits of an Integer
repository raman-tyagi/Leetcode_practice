class Solution {
public:
    int subtractProductAndSum(int n) {
        long long prod=1;
        int sum=0;
        while(n>0){
            int rem=n%10;
             prod=prod*rem;
            sum=sum+rem;
            n=n/10;
        }
        
        int result=prod-sum;
        return result;
        
    }
};
